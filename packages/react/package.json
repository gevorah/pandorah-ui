{
  "name": "@pandorah-ui/react",
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tsup && tailwindcss -i ./src/index.css -o ./dist/index.css --minify",
    "dev": "tsup --watch && tailwindcss -i ./src/index.css -o ./dist/index.css --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.16",
    "@lexical/history": "^0.15.0",
    "@lexical/link": "^0.15.0",
    "@lexical/list": "^0.15.0",
    "@lexical/react": "^0.15.0",
    "@lexical/rich-text": "^0.15.0",
    "@lexical/selection": "^0.15.0",
    "@lexical/table": "^0.15.0",
    "@lexical/utils": "^0.15.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lexical": "^0.15.0",
    "react-dropzone": "^14.2.3",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@pandorah-ui/tailwind-config": "workspace:*",
    "@pandorah-ui/tsconfig": "workspace:*",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tsup": "^8.0.2",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "react": "^18.0"
  }
}